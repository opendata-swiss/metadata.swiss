          pv:mappingClass "StandardKeyword" ;
          pv:mappingName "catalog"
      ], [
          sh:maxCount 1 ;
          sh:path dct:creator ;
          sh:severity sh:Violation ;
          pv:mappingClass "Agent" ;
          pv:mappingName "creator"
      ], [
          sh:path dcat:dataset ;
