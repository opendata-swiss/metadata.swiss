dcatap:Agent_Shape
    a sh:NodeShape ;
    sh:name "Agent"@en ;
    sh:property [
          sh:minCount 1 ;
          sh:nodeKind sh:Literal ;
          sh:path foaf:name ;
          sh:severity sh:Violation ;
          pv:mappingClass "SimpleMultiLang" ;
          pv:mappingName "name"
      ], [
          sh:maxCount 1 ;
          sh:path dct:type ;
          sh:severity sh:Violation ;
          pv:mappingClass "Standard" ;
          pv:mappingName "type"
      ], [
          sh:maxCount 1 ;
          sh:path foaf:homepage ;
          sh:severity sh:Violation ;
          pv:mappingClass "Standard" ;
          pv:mappingName "homepage"
      ], [
          sh:maxCount 1 ;
          sh:path foaf:mbox ;
          sh:severity sh:Violation ;
          pv:mappingClass "Standard" ;
          pv:mappingName "email"
      ], [
          sh:path dct:description ;
          sh:severity sh:Violation ;
          pv:mappingClass "SimpleMultiLang" ;
          pv:mappingName "description"
      ] ;

    sh:targetClass foaf:Agent .

dcatap:CatalogRecord_Shape
    a sh:NodeShape ;
    sh:name "Catalog Record"@en ;
    sh:property [
          sh:maxCount 1 ;
          sh:minCount 1 ;
          sh:node dcatap:DcatResource_Shape ;
          sh:path foaf:primaryTopic ;
          sh:severity sh:Violation
      ], [
          sh:maxCount 1 ;
          sh:minCount 1 ;
          sh:path dct:modified ;
          sh:severity sh:Violation ;
          sh:shape dcatap:DateOrDateTimeDataType_Shape ;
          pv:mappingClass "DateTime" ;
          pv:mappingName "modified"
      ], [
          sh:maxCount 1 ;
          sh:path dct:conformsTo ;
          sh:severity sh:Violation
      ], [
          sh:maxCount 1 ;
          sh:node dcatap:DateOrDateTimeDataType_Shape ;
          sh:path dct:issued ;
          sh:severity sh:Violation ;
          pv:mappingClass "DateTime" ;
          pv:mappingName "issued"
      ], [
          sh:maxCount 1 ;
          sh:path adms:status ;
          sh:severity sh:Violation
      ], [
          sh:path dct:language ;
          sh:severity sh:Violation
      ], [
          sh:maxCount 1 ;
          sh:path dct:source ;
          sh:severity sh:Violation
      ], [
          sh:nodeKind sh:Literal ;
          sh:path dct:title ;
          sh:severity sh:Violation
      ], [
          sh:nodeKind sh:Literal ;
          sh:path dct:description ;
          sh:severity sh:Violation
      ], [
          sh:maxCount 1 ;
          pv:mappingClass "Integer" ;
          pv:mappingName "revision_number"
      ], [
          sh:maxCount 1 ;
          pv:mappingClass "StandardKeyword" ;
          pv:mappingName "access_level"
      ], [
          sh:maxCount 1 ;
          pv:mappingClass "StandardKeyword" ;
          pv:mappingName "publication_status"
      ], [
          pv:mappingClass "Nested" ;
          pv:mappingName "revisions" ;
          pv:mappingProperty [
              sh:minCount 1 ;
              sh:maxCount 1 ;
              pv:mappingClass "Integer" ;
              pv:mappingName "revision_number"
          ], [
              sh:minCount 1 ;
              sh:maxCount 1 ;
              pv:mappingClass "StandardText" ;
              pv:mappingName "id"
          ], [
              sh:minCount 1 ;
              sh:maxCount 1 ;
              pv:mappingClass "DateTime" ;
              pv:mappingName "modified"
          ]
      ] ;
    sh:targetClass dcat:CatalogRecord .

dcatap:Catalog_Shape
